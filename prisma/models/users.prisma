model User {
  id          String   @id @default(uuid())
  email       String   @unique
  email_verified_at   DateTime?
  password    String
  last_login  DateTime?
  created_at  DateTime  @default(now())
  updated_at  DateTime  @updatedAt

  profile Profile?
  refreshToken RefreshToken[]
  passwordResetTokens PasswordResetToken[]


  @@map("users")
}